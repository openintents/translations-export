#: tmp_strings.xml:128(string)
msgid "Version not supported: %s"
msgstr "Η έκδοση δεν υποστηρίζεται : %s"

#: tmp_strings.xml:130(string)
msgid "Restore error."
msgstr "Σφάλμα επαναφοράς."

#: tmp_strings.xml:131(string)
msgid "Bad password."
msgstr "Λανθασμένος κωδικός."

#: tmp_strings.xml:132(string)
msgid "Restore Database"
msgstr "Επαναφορά Βάσης Δεδομένων"

#: tmp_strings.xml:133(string)
msgid "Are you sure you want to replace the current data?"
msgstr "Είστε σίγουρος ότι θέλετε να αντικαταστήσετε τα τρέχοντα δεδομένα;"

#: tmp_strings.xml:134(string)
msgid "Found %1$s passwords from backup dated %2$s."
msgstr ""
"Βρέθηκαν %1$s κωδικοί από το αντίγραφο ασφαλείας που χρονολογείται από %2$s."

#: tmp_strings.xml:135(string)
msgid "Complete. Passwords restored: %s"
msgstr "Ολοκλήρωση. Οι κωδικοί επαναφέρθηκαν: %s"

#: tmp_strings.xml:136(string)
msgid "copied to Clipboard"
msgstr "αντιγράφτηκε στο Πρόχειρο"

#: tmp_strings.xml:137(string)
msgid "Invalid website"
msgstr "Λανθασμένη ιστοσελίδα"

#: tmp_strings.xml:138(string)
msgid "Database Error"
msgstr "Σφάλμα Βάσης Δεδομένων"

#: tmp_strings.xml:139(string)
