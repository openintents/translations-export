msgid "MS Outlook Notes"
msgstr "MS Outlook Notları"

#: tmp_strings.xml:126(item)
msgid "Palm CSV"
msgstr "Palm CSV"

#: tmp_strings.xml:127(item)
msgid "JPilot Memo"
msgstr "JPilot Notu"

#: tmp_strings.xml:131(item)
msgid "MS Outlook Tasks"
