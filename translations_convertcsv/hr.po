#: tmp_strings.xml:64(string)
msgid "Select notepad CSV file"
msgstr "Odaberi blokče CSV datoteku"

#: tmp_strings.xml:65(string)
msgid "Select shopping list CSV file"
msgstr "Odaberi listu za kupovinu CSV datoteku"

