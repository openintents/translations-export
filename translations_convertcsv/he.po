#: tmp_strings.xml:56(string)
msgid "Export"
msgstr "ייצוא"

#: tmp_strings.xml:57(string)
msgid "Error writing to file"
msgstr "אירעה שגיאה בכתיבה לקובץ"

#: tmp_strings.xml:58(string)
