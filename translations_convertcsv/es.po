msgstr "OI Lista de Compras"

#: tmp_strings.xml:100(string)
msgid "Import/export settings for OI Shopping list."
msgstr "Configuración de Importación/Exportación para OI Lista de Compras"

#: tmp_strings.xml:102(string)
msgid "Converting..."
msgstr "Convirtiendo..."

#: tmp_strings.xml:103(string)
