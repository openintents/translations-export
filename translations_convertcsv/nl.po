#: tmp_strings.xml:135(item) tmp_strings.xml:149(item)
msgid "DUPLICATE - Allow duplicates of records."
msgstr "DUPLICEREN - sta duplicaties van gegevens toe."

#: tmp_strings.xml:136(item) tmp_strings.xml:150(item)
msgid "\"KEEP - Don't overwrite existing records.\""
msgstr "\"HOUDEN - Overschrijf geen bestaande gegevens.\""

#: tmp_strings.xml:137(item)
msgid "Overwrite - Overwrite existing records."
msgstr "Overschrijf - Overschrijf bestaande gegevens."

#: tmp_strings.xml:138(item)
msgid "Restore - DELETE ALL, then import."
msgstr "Herstellen - VERWIJDER ALLES, dan importeren."

#: tmp_strings.xml:142(item) tmp_strings.xml:154(item)
msgid "DUPLICATE - Add a second copy of a record if it already exists."
msgstr ""
"DUPLICEREN - Voeg een tweede kopie toe van een document als het al bestaat."

#: tmp_strings.xml:143(item) tmp_strings.xml:155(item)
msgid ""
"\"KEEP - Don't overwrite an existing record, ignore the one in the CSV file."
"\""
msgstr ""
"\"BEHOUDEN - Overschrijf geen bestaande gegevens, negeer degene in het CSV "
"bestand.\""

#: tmp_strings.xml:144(item)
msgid "Overwrite - A record in the CSV will OVERWRITE an existing record."
msgstr ""
"Overschrijven - Een gegeven in het CSV bestand zal een bestaand gegeven "
"OVERSCHRIJVEN."

#: tmp_strings.xml:145(item)
msgid ""
"Restore Backup - DELETE ALL YOUR EXISTING RECORDS, then import the CSV file."
msgstr ""
"Herstel reservekopie - VERWIJDER AL UW BESTAANDE GEGEVENS, importeer dan het "
"CSV bestand."
