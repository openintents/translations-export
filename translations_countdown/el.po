#: tmp_strings.xml:39(string)
msgid "Set action"
msgstr "Ορισμός ενέργειας"

#: tmp_strings.xml:41(string)
msgid "e.g. pizza"
msgstr "π.χ. πίτσα"

#. Title of the activity for setting (editing) the countdown.
#: tmp_strings.xml:44(string)
msgid "Set countdown"
msgstr "Ορισμός αντίστροφης μέτρησης"

