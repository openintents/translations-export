msgid "5 seconds"
msgstr "5 sekund"

#: tmp_strings.xml:162(item)
msgid "10 seconds"
msgstr "10 sekund"

#: tmp_strings.xml:163(item)
msgid "30 seconds"
msgstr "30 sekund"

#: tmp_strings.xml:164(item)
msgid "1 minute"
msgstr "1 minuta"

