#: tmp_strings.xml:73(string)
msgid "Ring"
msgstr "טבעת"

#. Checkbox label whether to vibrate.
#: tmp_strings.xml:76(string)
msgid "Vibrate"
msgstr "רטט"

#. Checkbox label whether to flash light for notifications.
#: tmp_strings.xml:79(string)
msgid "Light"
msgstr "בהיר"

