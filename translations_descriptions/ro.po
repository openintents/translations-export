#: tmp_strings.xml:26(string)
msgid "Features:"
msgstr ""

#: tmp_strings.xml:27(listitem)
msgid "Show list of files with thumbnails for images."
msgstr ""

#: tmp_strings.xml:28(listitem)
msgid "Move, copy, rename, and delete files."
msgstr "Mută, copiază, redenumește și șterge fișiere."

#: tmp_strings.xml:29(listitem)
msgid "Create and delete directories (folders)."
msgstr "Crează și șterge dosare."

#: tmp_strings.xml:30(listitem)
msgid "Send files by email."
msgstr "Trimite fișiere prin email."

#: tmp_strings.xml:31(listitem)
msgid ""
"Acts as an extension to other applications, like GMail, to attach files."
msgstr ""
"Acționează ca o extensie pentru alte aplicații, GMail de exemplu, pentru a "
"atașa fișiere."

#: tmp_strings.xml:32(listitem)
msgid "Support for many file endings and mime types."
msgstr ""

#: tmp_strings.xml:33(listitem)
msgid ""
"Supports PICK_FILE and PICK_DIRECTORY intents so that other applications can "
"use OI File Manager."
msgstr ""

